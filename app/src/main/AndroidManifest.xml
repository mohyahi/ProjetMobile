<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projet">

    <uses-permission
        android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".InscriptionActivity"></activity>
        <activity android:name=".ConnexionActivity"></activity>
        <activity android:name=".ConfirmPasswordActivity"></activity>
        <activity android:name=".listMedecinActivity"></activity>
        <activity android:name=".RechercheMedecinActivity"></activity>
        <activity android:name=".AccueilActivity"></activity>
        <activity android:name=".AjoutTraitantActivity"></activity>
        <activity android:name=".listTraitantsActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>